pipeline {
    agent any

    triggers {
        githubPush()
    }

    stages {
        stage('Build') {
            steps {
                echo "Building.."
                sh 'echo "doing build stuff.."'
            }
        }

        stage('Test') {
            steps {
                echo "Testing.."
                sh 'echo "doing test stuff.."'
            }
        }

        stage('Deliver') {
            steps {
                echo "Deliver...."
                sh 'echo "doing delivery stuff..hello its working"'
            }
        }
    }
}
